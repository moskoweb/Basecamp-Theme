							<td style="direction:ltr;font-size:0px;padding:20px 0;padding-top:0;text-align:center;vertical-align:top;">
								<div data-slot-container="1" class="mj-column-per-100 outlook-group-fix" style="font-size:13px;text-align:left;direction:ltr;display:inline-block;vertical-align:top;width:100%;">
									<table background="#FFF" border="0" cellpadding="0" cellspacing="0" role="presentation" style="background-color:#FFF;vertical-align:top;" width="100%">
										<tr>
											<td style="font-size:0px;padding:10px 25px;word-break:break-word;">
